{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "90"
        },
        "modules": false
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-class-properties",
    "@babel/plugin-transform-runtime"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-refresh/babel"
      ]
    }
  }
}
